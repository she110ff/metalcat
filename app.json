{
  "expo": {
    "name": "MetalCat",
    "slug": "metalcat-first",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/metalcat_logo.png",
    "scheme": "metalcat",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/metalcat_logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.metalcatapp.metalcat",
      "deploymentTarget": "13.0",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/metalcat_logo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.metalcatapp.metalcat"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/NanumGothic.otf",
            "./assets/fonts/NanumGothicBold.otf",
            "./assets/fonts/SpaceMono-Regular.ttf"
          ]
        }
      ],
      "expo-asset",
      "expo-web-browser",
      [
        "expo-notifications",
        {
          "icon": "./assets/images/metalcat_logo.png",
          "color": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "19829544-dd83-47d5-8c48-ffcdc913c8b1"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/19829544-dd83-47d5-8c48-ffcdc913c8b1"
    },
    "owner": "metalcatapp"
  }
}
