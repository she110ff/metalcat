{
  "expo": {
    "name": "MetalCat",
    "slug": "metalcat-first",
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/images/metalcat_logo.png",
    "scheme": "metalcat",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/metalcat_logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.metalcatapp.metalcat",
      "buildNumber": "1",
      "deploymentTarget": "13.0",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSCameraUsageDescription": "경매 물품의 사진을 촬영하기 위해 카메라 접근이 필요합니다.",
        "NSPhotoLibraryUsageDescription": "기존 사진을 업로드하기 위해 사진 라이브러리 접근이 필요합니다.",
        "NSPhotoLibraryAddUsageDescription": "촬영한 사진을 저장하기 위해 사진 라이브러리 접근이 필요합니다."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/metalcat_logo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.metalcatapp.metalcat",
      "versionCode": 1,
      "googleServicesFile": "./google-services.json",
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/NanumGothic.otf",
            "./assets/fonts/NanumGothicBold.otf",
            "./assets/fonts/SpaceMono-Regular.ttf"
          ]
        }
      ],
      "expo-asset",
      "expo-web-browser",
      [
        "expo-notifications",
        {
          "icon": "./assets/images/metalcat_logo.png",
          "color": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "19829544-dd83-47d5-8c48-ffcdc913c8b1"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "enabled": true,
      "url": "https://u.expo.dev/19829544-dd83-47d5-8c48-ffcdc913c8b1",
      "fallbackToCacheTimeout": 0,
      "checkAutomatically": "ON_ERROR_RECOVERY"
    },
    "owner": "metalcatapp"
  }
}
